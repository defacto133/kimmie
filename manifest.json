{
	"manifest_version": 2,
    "version": "0.1",
    "name": "Kimmie",
    "description": "Replaces the \"Jong Un\" in Kim Jong Un's name to \"Kardashian\".",
    "content_scripts": [
        {
            "matches": ["*://*/*"],
            "js": ["content_script.js"],
            "run_at": "document_end"
        }],
    "icons": { "16": "icon16.png",
               "48": "icon48.png",
              "128": "icon128.png" }
}
